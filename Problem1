import java.util.*;
import java.lang.*;
import java.io.*;

class Easy
{
	public static void main (String[] args) throws java.lang.Exception
	{
		int s = 0;
		for(int i = 1; i < 1000; i++)
			if( i%3 == 0 || i%5 == 0 )
				s += i;
		System.out.println(s);
	}
}
